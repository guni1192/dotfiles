vim: set ft=xdefaults:
!------------------------------------------------------------------------------
! Xft settings                                                              {{{
!------------------------------------------------------------------------------

Xft.dpi:       96
Xft.antialias: true
Xft.rgba:      rgb
Xft.hinting:   true
Xft.hintstyle: hintslight

!---------------------------------------------------------------------------}}}
! URxvt settings                                                            {{{
!------------------------------------------------------------------------------

! For showing img in ranger with w3mimgdisplay, disable this
! URxvt*.depth:          32
URxvt.geometry:        90x35
URxvt*.shading:        110
URxvt*.transparent:    false
! URxvt*.transparent:    true
URxvt.tintColor:       #303030
! URxvt*.shading:        110
! URxvt.tintColor:       #303030
URxvt*fading:          0
! URxvt.urgentOnBell:    true
URxvt*visualBell:      false
URxvt*loginShell:      true
URxvt.saveLines:       2000
URxvt.internalBorder:  0
URxvt.lineSpace:       0
URxvt.intensityStyles: true
URxvt.perl-ext-common: default,selection
URxvt.url-launcher:    /usr/bin/xdg-open
URxvt.mathcer.button:  1

! Fonts
URxvt.allow_bold:      false
URxvt*font:            xft:Inconsolata for Powerline:size=13,xft:Migu 1M:size=13
! Fix font space
URxvt*letterSpace:     0

! Scrollbar
URxvt.scrollBar:       false
URxvt.scrollStyle:     rxvt
URxvt.scrollBar_right: true

! Cursor
URxvt.cursorBlink:     true
URxvt.cursorUnderline: false

! Pointer
URxvt.pointerBlank:    true

! iso 14755 disable
URxvt*iso14755:        false

!---------------------------------------------------------------------------}}}
