#!/usr/bin/zsh
# Path to your oh-my-zsh installation.
#export ZSH=$HOME/.oh-my-zsh

export TERM="xterm-256color"

#ZSH_THEME="agnoster"


export PATH="/Users/IIGUNI/.rbenv/shims:/Users/IIGUNI/.rbenv/bin:/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/texlive/2016/bin/x86_64-darwin/tlmgr"

export PYTHONPATH=/usr/local/lib/python3.5/site-packages
if [[ -r $PYTHONPATH/powerline/bindings/zsh/powerline.zsh ]]; then
    powerline-daemon -q
    source $PYTHONPATH/powerline/bindings/zsh/powerline.zsh
fi
#source $ZSH/oh-my-zsh.sh

alias tmux="tmux -2"
alias emacs="/usr/local/Cellar/emacs/25.1//Emacs.app/Contents/MacOS/Emacs -nw"
alias e="/usr/local/Cellar/emacs/25.1//Emacs.app/Contents/MacOS/Emacs -nw"
alias ei='e ~/.emacs.d/init.el'
alias python='python3'
alias pip='pip3'
# unalias ls
# unalias la
# unalias ll
alias ls='/bin/ls -hG'
alias la='/bin/ls -AGh'
alias ll='/bin/ls -AhlG'
cdls ()
{
    \cd "$@" && ls
}
alias cd='cdls'
alias grep='grep --color'
alias df='df -h'

